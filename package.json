{
  "name": "mirai-notes2",
  "version": "1.0.0",
  "description": "My super-excellent Nuxt.js project",
  "author": "kght6123",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "yarn lint",
    "dev": "nuxt",
    "build": "nuxt build && rm -R functions/public/* && cp -R functions/nuxt/dist/ functions/public && cp -R static/* functions/public && mv functions/public/client functions/public/_nuxt",
    "start": "nuxt start",
    "run": "nuxt build && nuxt start",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@mdi/font": "^3.7.95",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/localforage": "^1.0.2",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/style-resources": "^0.1.2",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "firebase": "6.3.4",
    "firebaseui": "4.1.0",
    "jquery": "^3.4.1",
    "nuxt": "^2.0.0",
    "nuxt-fire": "^2.0.0",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.15.0",
    "tui-editor": "^1.4.4-alpha.0",
    "vee-validate": "^2.2.11"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "pug": "2.0.3",
    "pug-plain-loader": "^1.0.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
